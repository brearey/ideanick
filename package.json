{
  "name": "ideanick",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "b": "pnpm --filter @ideanick/backend",
    "w": "pnpm --filter @ideanick/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently 'pnpm b types' 'pnpm w types'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "format": "pnpm -r --parallel format",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-dom": "^1.52.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  }
}